# Atm, there is no built-in multiplication
# And also no cycles, all by hand

fun mul(a, b) {
   if a == 1 {
      return b; 
   } else {
      return b + mul(a-1, b);
   }
}

print("Here");

fun factorial(n) {
   if n == 0 {
      return 1;
   } else {
      return mul(n, factorial(n-1));
   }
}

print("Here");

print(factorial(3));
