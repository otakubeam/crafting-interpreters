# Atm, there is no built-in multiplication
# And also no cycles, all by hand

fun mul(a, b) {
   if a == 1 {
      b 
   } else {
      b + mul(a-1, b)
   }
}

print("Here");

fun factorial(n) {
   if n == 0 {
      1
   } else {
      mul(n, factorial(n-1))
   }
}

print("Here");

print(factorial(3));
